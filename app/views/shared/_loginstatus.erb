<div>
  <%% if(!signedIn) { %>
  <p>
    <%= link_to 'Login or Sign Up!', new_user_session_path, {:class=> "login"} %>
  </p>
  <%% } else { %>
    Welcome, <%%= signInName %> | <a class="purchases" href="#">My Purchases</a> | 
    <%= link_to "logout", destroy_user_session_path, {:class=> "logout"} %>
  <%% } %>
</div>